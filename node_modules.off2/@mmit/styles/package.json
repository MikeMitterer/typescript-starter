{
  "name": "@mmit/styles",
  "version": "0.1.33",
  "description": "Styles for simple MobiAd-Tools",
  "main": "lib/index.js",
  "unpkg": "css/mobiad.css",
  "style": "css/mobiad.min.css",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf css dist",
    "serve": "NODE_ENV=development webpack server --config webpack.web.js --mode development",
    "start": "yarn serve --open",
    "build": "yarn clean && yarn build:images && yarn build:sass && yarn build:autoprefixer && yarn build:min && yarn build:ts",
    "build:ts": "tsc --rootDir $(pwd)/src --project tsconfig.lib.json --declaration --outDir $(pwd)/lib",
    "build:sass": "sass -c --source-map src/styles/mobiad.scss css/mobiad.css",
    "build:images": "mkdir -p images && cp -a src/images/* images",
    "build:autoprefixer": "postcss --use autoprefixer --map false --output css/mobiad.css css/mobiad.css",
    "build:min": "cleancss -o css/mobiad.min.css css/mobiad.css",
    "build:web": "yarn run clean && NODE_ENV=development webpack --config webpack.web.js --mode development",
    "build:web:prod": "yarn run clean && NODE_ENV=production webpack --config webpack.web.js --mode production",
    "deploy": "yarn version --new-version patch && yarn publish --access restricted --non-interactive"
  },
  "author": "Mike Mitterer",
  "license": "UNLICENSED",
  "files": [
    "css",
    "images",
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeMitterer/scss-styles"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@mmit/logging": "^0.1.6",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "beautify-html-webpack-plugin": "^1.0.3",
    "clean-css-cli": "^5.3.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "^5.2.6",
    "date-fns": "2.22.1",
    "ejs-compiled-loader": "^3.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^1.6.2",
    "path": "^0.12.7",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "tslint": "^6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-livereload-plugin": "^3.0.1"
  }
}
