{
  "extends": [
    "tslint:recommended"
  ],
  
  "rules": {
    // "no-console": false
    "no-trailing-whitespace": {
      "severity": "off"
    },
    "one-line": [true, "check-catch", "check-finally", "check-open-brace", "check-whitespace"],
    "quotemark": false,
    "explicit-override": [ true, "decorator" ],
    "object-literal-sort-keys": false,
    "no-any": true
  }
}
