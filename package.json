{
    "name": "@mmit/typescript-starter",
    "author": "Mike Mitterer",
    "version": "0.6.33",
    "description": "This is a starter project for typescript projects",
    "keywords": [
        "typescript"
    ],
    "license": "MIT",
    "type-check": "tsc",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "pkg:version": "echo $(jq -r -j '.name' < package.json;echo -n ': ';jq -r -j '.version' < package.json)",
        "update": "yarn update:browser",
        "update:browser": "browserslist --update-db",
        "clean": "rm -rf lib dist bin &&  rm -rf node_modules/@types/istanbul-reports && jest --clearCache && rm -rf .ts.cache && rm -rf .rollup.cache && rm -f .tsbuildinfo",
        "gen:barrels": "barrelsby --singleQuotes --delete -d src/main --exclude ConfigLog4j --exclude 'events/utils' -l top -s flat",
        "serve": "NODE_ENV=development webpack serve --config webpack.web.js --mode development",
        "start": "yarn serve --open",
        "tc": "tsc --noEmit --project tsconfig.lib.json",
        "tc:watch": "npm run tc -- --watch",
        "lint": "tslint --fix --project tsconfig.json",
        "test": "yarn run gen:barrels && jest --clearCache && yarn run test:unit && yarn run test:integration",
        "test:unit": "jest --colors test/unit",
        "test:integration": "jest --colors --runInBand --detectOpenHandles test/integration",
        "test:e2e": "E2E_TEST='true' jest --colors --runInBand --detectOpenHandles test/e2e",
        "build": "rm -rf lib && yarn run gen:barrels && yarn run build:ts",
        "build:ts": "tsc --rootDir $(pwd)/src/main --project tsconfig.lib.json --declaration --outDir $(pwd)/lib",
        "build:ts:watch": "yarn build:ts --watch",
        "build:types": "rm -rf lib && yarn build:ts --emitDeclarationOnly",
        "build:js": "rm -rf lib && NODE_ENV=development webpack --config webpack.ts.js --mode development",
        "build:js:prod": "rm -rf lib && NODE_ENV=production webpack --config webpack.ts.js --mode production",
        "build:babel": "rm -rf lib && REQUIRE_TARGET='browser' babel src/main --out-dir lib --extensions \".ts,.tsx\" --source-maps",
        "build:node": "rm -rf bin && NODE_ENV=development webpack --config webpack.node.js --mode development",
        "build:node:prod": "NODE_ENV=production webpack --config webpack.node.js --mode production",
        "build:web": "rm -rf dist && NODE_ENV=development webpack --config webpack.web.js --mode development",
        "build:web:prod": "rm -rf dist && NODE_ENV=production webpack --config webpack.web.js --mode production",
        "deploy": "yarn version --new-version patch && yarn publish --non-interactive",
        "doc": "typedoc --out doc --tsconfig tsconfig.lib.json src/main/",
        "prettier": "onchange './src/**/*.ts'  -- prettier --write {{changed}}"
    },
    "files": [
        "lib",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/typescript-training"
    },
    "dependencies": {
        "@mmit/logging": "0.1.9",
        "date-fns": "^2.30.0",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.10",
        "@babel/core": "^7.22.11",
        "@babel/node": "^7.22.10",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/plugin-transform-runtime": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "@babel/preset-typescript": "^7.22.11",
        "@babel/runtime": "^7.22.11",
        "@mmit/muni": "^0.0.2",
        "@mmit/styles": "^0.1.33",
        "@types/chai": "^4.3.5",
        "@types/core-js": "^2.5.4",
        "@types/expect-puppeteer": "5.0.3",
        "@types/jest": "^29.5.4",
        "@types/node": "^20.5.7",
        "@types/shelljs": "^0.8.12",
        "@types/ws": "^8.5.5",
        "autoprefixer": "^10.4.15",
        "babel-jest": "^29.6.4",
        "babel-loader": "^9.1.3",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "babel-preset-modern-browsers": "^15.0.2",
        "barrelsby": "^2.8.0",
        "chai": "^4.3.8",
        "clean-terminal-webpack-plugin": "^3.0.0",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "ejs-compiled-loader": "^3.1.0",
        "file-loader": "^6.2.0",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.3",
        "html-webpack-string-replace-plugin": "^1.1.0",
        "jest": "^29.6.4",
        "jest-cli": "^29.6.4",
        "jest-environment-jsdom": "^29.6.4",
        "jest-extended": "^4.0.1",
        "jest-haste-map": "^29.6.4",
        "jest-puppeteer": "^9.0.0",
        "jest-resolve": "^29.6.4",
        "mini-css-extract-plugin": "^2.7.6",
        "postcss": "^8.4.29",
        "postcss-loader": "^7.3.3",
        "puppeteer": "^21.1.1",
        "sass": "^1.66.1",
        "sass-loader": "^13.3.2",
        "semver": "^7.5.4",
        "shelljs": "^0.8.4",
        "source-map-loader": "^4.0.1",
        "style-loader": "^3.3.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.0.0",
        "tslint": "^6.1.3",
        "tslint-consistent-codestyle": "^1.16.0",
        "tslint-loader": "^3.5.4",
        "tslint-override": "^1.0.1",
        "typedoc": "^0.25.0",
        "typescript": "^5.2.2",
        "url-loader": "^4.1.1",
        "webpack": "5.88.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.7.4",
        "webpack-livereload-plugin": "^3.0.1",
        "webpack-node-externals": "^3.0.0"
    }
}
